{
  "consumer": {
    "name": "ShoppingListFront"
  },
  "interactions": [
    {
      "description": "a request to delete a list",
      "providerState": "has list to delete",
      "request": {
        "method": "DELETE",
        "path": "/api/v1/itemsList/1"
      },
      "response": {
        "status": 204
      }
    },
    {
      "description": "a request to get a list",
      "providerState": "has list to get",
      "request": {
        "method": "GET",
        "path": "/api/v1/itemsList/1"
      },
      "response": {
        "body": {
          "id": 1,
          "items": [
            {
              "bought": true,
              "comment": "For juice",
              "id": 2,
              "name": "Orange"
            }
          ],
          "name": "Shopping list"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "$.body": {
            "match": "type"
          },
          "$.body.id": {
            "match": "type"
          },
          "$.body.items": {
            "match": "type",
            "min": 1
          },
          "$.body.items[*].bought": {
            "match": "type"
          },
          "$.body.items[*].comment": {
            "match": "type"
          },
          "$.body.items[*].id": {
            "match": "type"
          },
          "$.body.items[*].name": {
            "match": "type"
          },
          "$.body.name": {
            "match": "type"
          }
        },
        "status": 200
      }
    },
    {
      "description": "a request to update a list",
      "providerState": "has list to update",
      "request": {
        "body": {
          "name": "Shopping List 1"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "method": "PUT",
        "path": "/api/v1/itemsList/1"
      },
      "response": {
        "status": 204
      }
    },
    {
      "description": "a request to get all lists",
      "providerState": "has lists to get",
      "request": {
        "method": "GET",
        "path": "/api/v1/"
      },
      "response": {
        "body": [
          {
            "id": 45326,
            "name": "Shopping list"
          }
        ],
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "$.body": {
            "match": "type",
            "min": 1
          },
          "$.body[*].id": {
            "match": "type"
          },
          "$.body[*].name": {
            "match": "type"
          }
        },
        "status": 200
      }
    },
    {
      "description": "a request to add a list",
      "providerState": "there are no lists",
      "request": {
        "body": {
          "name": "Shopping list"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "$.body": {
            "match": "type"
          },
          "$.body.name": {
            "match": "type"
          }
        },
        "method": "POST",
        "path": "/api/v1/itemsList"
      },
      "response": {
        "body": {
          "id": 45326
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "$.body": {
            "match": "type"
          },
          "$.body.id": {
            "match": "type"
          }
        },
        "status": 201
      }
    }
  ],
  "metadata": {
    "pact-js": {
      "version": "11.0.2"
    },
    "pactRust": {
      "ffi": "0.4.0",
      "models": "1.0.4"
    },
    "pactSpecification": {
      "version": "2.0.0"
    }
  },
  "provider": {
    "name": "ShoppingListBack"
  }
}